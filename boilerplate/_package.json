{
  "name": "{{name}}",
  "version": "1.0.0",
  "description": "{{description}}",
  "private": true,
  "egg": {
    "declarations": true,
    "framework": "@bt/bt-egg"
  },
  "dependencies": {
    "@bt/bt-egg": "^{{btEggVersion}}",
    "@bt/bt-sequelize-tables-prerelease-v2": "^3.0.0",
    "egg-router-plus": "^1.3.1",
    "egg-scripts": "^2.5.0"
  },
  "devDependencies": {
    "autod": "^3.0.1",
    "autod-egg": "^1.1.0",
    "egg-bin": "^4.11.0",
    "egg-ci": "^1.11.0",
    "egg-mock": "^3.21.0",
    "eslint": "^5.13.0",
    "eslint-config-egg": "^7.1.0"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "scripts": {
    "start": "egg-scripts start --title=egg-server-{{name}}",
    "stop": "egg-scripts stop --title=egg-server-{{name}}",
    "dev": "egg-bin dev | node_modules/.bin/bunyan",
    "debug": "egg-bin debug | node_modules/.bin/bunyan",
    "test": "npm run lint -- --fix && npm run test-local",
    "test-local": "egg-bin test",
    "cov": "egg-bin cov",
    "lint": "eslint .",
    "ci": "npm run lint && npm run cov",
    "autod": "autod"
  },
  "ci": {
    "version": "10"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "{{author}}",
  "license": "MIT",
  "main": ".autod.conf.js",
  "directories": {
    "test": "test"
  }
}
